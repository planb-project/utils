{
    "name": "planb/utils",
    "description": "Libreria de uso comun para proyectos php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Manuel Pantoja",
            "email": "jmpantoja@gmail.com",
            "homepage": "https://github.com/planb-project/"
        }
    ],
    "autoload": {
        "psr-4": {
            "PlanB\\Utils\\": "src/Utils"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "apcu-autoloader": true,
        "bin-dir": "bin"
    },
    "extra": {
        "github_organization": "planb-project",
        "github_repository": "utils"
    },
    "require": {
        "marc-mabe/php-enum": "^3.1",
        "ramsey/uuid": "^3.8",
        "webmozart/assert": "^1.4"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "ciaranmcnulty/phpspec-typehintedmethods": "^4.0",
        "leanphp/phpspec-code-coverage": "dev-upgrade",
        "memio/spec-gen": "^0.9.0",
        "phpspec/phpspec": "^5.1",
        "symfony/var-dumper": "^4.2"
    },
    "repositories": [{
            "type": "vcs",
            "url": "https://github.com/shulard/phpspec-code-coverage.git"
        }]
}
